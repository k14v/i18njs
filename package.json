{
  "name": "i18njs",
  "version": "0.0.1",
  "description": "Library to internationalize literals",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text ava --verbose",
    "test:watch": "ava --watch",
    "build": "babel src --out-dir lib",
    "watch": "npm run build -- --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "example": "./node_modules/.bin/babel-node examples/basic.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rubeniskov/i18njs.git"
  },
  "keywords": [
    "i18n",
    "l10n"
  ],
  "author": "",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "ava": {
    "files": [
      "**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "make-plural": "^4.3.0",
    "math-interval-parser": "^2.0.0",
    "messageformat": "^2.0.4",
    "mustache": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-rc.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.4",
    "nyc": "^13.0.1",
    "sinon": "^7.1.1"
  },
  "peerDependencies": {}
}
